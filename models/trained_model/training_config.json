{
  "data_dir": "/content/data",
  "output_dir": "/content/outputs/models/floormind_baseline",
  "model_name": "runwayml/stable-diffusion-v1-5",
  "resolution": 512,
  "train_batch_size": 4,
  "eval_batch_size": 2,
  "num_epochs": 10,
  "learning_rate": 1e-05,
  "lr_scheduler": "cosine",
  "lr_warmup_steps": 500,
  "gradient_accumulation_steps": 2,
  "max_grad_norm": 1.0,
  "num_train_timesteps": 1000,
  "noise_schedule": "linear",
  "prediction_type": "epsilon",
  "save_steps": 500,
  "eval_steps": 250,
  "logging_steps": 50,
  "mixed_precision": "fp16",
  "dataloader_num_workers": 2,
  "enable_xformers": true,
  "gradient_checkpointing": true,
  "use_8bit_adam": false
}