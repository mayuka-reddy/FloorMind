    "                total_loss = standard_loss + constraint_losses['total_constraint_loss']\n",
    "                \n",
    "                # Backward pass\n",
    "                optimizer.zero_grad()\n",
    "                total_loss.backward()\n",
    "                optimizer.step()\n",
