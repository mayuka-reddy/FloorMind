    "    def calculate_clip_score(self, image: Image.Image, text: str) -> float:\n",
    "        \"\"\"Calculate CLIP similarity score between image and text\"\"\"\n",
    "        if self.clip_model is None:\n",
    "            # Mock CLIP score for demonstration\n",
